{
  "name": "atx_health_score",
  "version": "1.0.0",
  "description": "atx_health_score",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@urbica/react-map-gl": "^1.8.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "mapbox-gl": "^1.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "react": "^16.8.6",
    "react-bootstrap-table-next": "^3.1.7",
    "react-bootstrap-table2-filter": "^1.1.10",
    "react-bootstrap-table2-paginator": "^2.0.7",
    "react-bootstrap-table2-toolkit": "^2.0.1",
    "react-dom": "^16.8.6",
    "react-google-login": "^5.0.5",
    "react-iframe": "^1.8.0",
    "react-map-gl": "^5.0.9",
    "react-moment": "^0.9.2",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.1"
  }
}
